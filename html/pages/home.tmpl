{{define "main"}}
<header>
	<span><a href="/">
		<img
			id="evids"
			src="/logo.png"
			alt="EVIDS"
			onmouseover="evids.src='/bold-logo.png'"
			onmouseout="evids.src='/logo.png'"
		></img>
	</a></span>
</header>
<main>
	<ol class="heading">
		<li class="name">NAME</li>
		<li class="time">TIME</li>
	</ol>
	{{ range .}}
	<a href="{{.Path}}">
		<ol class="entry">
			<li class="name">{{.Name}}</li>
			<li class="time">{{.Time}}</li>
		</ol>
	</a>
	{{end}}
</main>
{{end}}
